{
  "name": "htp1-telnet-bridge",
  "version": "0.1.0",
  "description": "Bridge to the HTP-1 processor for Savant",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/bwerther/htp1-telnet-bridge",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "start": "NODE_PATH=./build node build/index.js",
    "build": "tsc -p .",
    "dev": "nodemon --inspect=0.0.0.0 --htp1Address=192.168.1.160 --telnetPort=4000",
    "docker-dev": "nodemon --inspect=0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^16.4.14",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@types/jsonpath": "^0.2.0",
    "@types/minimist": "^1.2.2",
    "@types/ws": "^7.4.7",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "fast-json-patch": "^3.0.0-1",
    "jsonpath": "^1.1.1",
    "minimist": "^1.2.5",
    "ws": "^8.0.0"
  }
}
